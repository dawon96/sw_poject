<div class="container">
    <div class="row">
        <div class="col s12">
            <% @posts.each do |post| %>
                <%= render partial: "post", object: post %>
            <% end %>
            <div class="fixed-action-btn">
                <a class="btn-floating btn-large waves-effect waves-light red" href="/posts/new"><i class="material-icons">add</i></a>
            </div>
        </div>
    </div>
</div>